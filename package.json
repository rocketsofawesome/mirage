{
  "name": "@rocketsofawesome/mirage",
  "version": "0.1.524",
  "main": "./build/index.js",
  "dependencies": {
    "accounting": "^0.4.1",
    "cloudinary-core": "^2.6.3",
    "gsap": "^2.1.3",
    "instafeed.js": "^1.4.1",
    "jspolyfill-array.prototype.find": "^0.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-image-magnify": "^2.7.4",
    "react-input-mask": "^2.0.4",
    "react-markdown": "^4.0.6",
    "react-responsive": "^1.1.3",
    "react-sizeme": "^2.5.2",
    "react-slick": "^0.23.2",
    "react-stripe-elements": "^3.0.0",
    "react-transition-group": "^1.2.1",
    "styled-components": "^3.2.6"
  },
  "scripts": {
    "start": "export NODE_ENV=development && ./node_modules/.bin/styleguidist server --open --max_old_space_size=4096",
    "build": "export NODE_ENV=development && ./node_modules/.bin/styleguidist build",
    "predeploy": "node ./scripts/build.js; npm run build",
    "deploy": "gh-pages -d styleguide --user 'ROAPairingStation <engineering@rocketsofawesome.com>'",
    "prepublishOnly": "npm run deploy",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "npm test -- -coverage -u",
    "test:badges": "npm run test:coverage  && jest-coverage-badges"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "contentful": "^7.6.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "2.29.0",
    "install": "^0.12.1",
    "jest": "^20.0.4",
    "jest-coverage-badges": "^1.1.2",
    "jest-enzyme": "^7.0.0",
    "jest-styled-components": "^6.2.1",
    "npm": "^6.4.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-dev-utils": "^5.0.1",
    "react-styleguidist": "^7.3.11",
    "react-test-renderer": "^16.5.2",
    "resolve": "1.6.0",
    "rollup": "^0.65.2",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-node-externals": "^1.7.2",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/theme/**/*.{js,jsx,mjs}",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/**/index.{js,jsx,mjs}"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^SRC(.*)$": "<rootDir>/src$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "keywords": [
    "react",
    "styled-components",
    "react-component",
    "rockets-of-awesome"
  ],
  "babel": {
    "presets": [
      "env",
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mealeyst/mirage.git"
  },
  "description": "[Live Demo of the Pattern Library](https://rocketsofawesome.github.io/mirage/)",
  "bugs": {
    "url": "https://github.com/mealeyst/mirage/issues"
  },
  "homepage": "https://rocketsofawesome.github.io/mirage/",
  "author": "Shawn Mealey",
  "license": "MIT"
}
